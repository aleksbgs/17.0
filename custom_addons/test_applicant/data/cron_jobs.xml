<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_mark_done" model="ir.cron">
            <field name="name">Mark Test Applicants as Done</field>
            <field name="model_id" ref="model_test_applicant"/>
            <field name="state">code</field>
            <field name="code">model.cron_mark_done()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>